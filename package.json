{
  "name": "petrel.interviewer",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --configuration=production",
    "serve-staging": "ng serve --configuration=staging -o",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-headless-ci-only": "ng test --browsers ChromiumNoSandbox",
    "lint": "ng lint",
    "pretty": "pretty-quick",
    "pretty-all": "prettier --write \"src/**/*.ts\" \"src/**/*.html\" \"src/**/*.scss\" \"src/**/*.json\" \"e2e/**/*.ts\" \"tools/**/*.ts\" \"*.json\"",
    "bundle:analyze": "ng build --stats-json && webpack-bundle-analyzer dist/petrel.interviewer/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^18.0.1",
    "@angular/animations": "~18.0.0",
    "@angular/cdk": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@auth0/auth0-angular": "^2.2.3",
    "@ng-select/ng-select": "^13.2.0",
    "@popperjs/core": "^2.11.8",
    "@sgratzl/chartjs-chart-boxplot": "^4.2.8",
    "angular-auth-oidc-client": "^17.0.0",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.2",
    "chart.js": "^4.4.1",
    "husky": "^7.0.4",
    "karma-spec-reporter": "^0.0.36",
    "ngx-cookie-service": "^18.0.0",
    "ngx-google-analytics": "^15.0.0",
    "ngx-markdown": "^18.0.0",
    "ngx-spinner": "^17.0.0",
    "oidc-client": "^1.11.5",
    "prettier": "^3.2.5",
    "puppeteer": "^22.10.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^18.0.0-alpha.5",
    "@angular-eslint/eslint-plugin": "^18.0.0-alpha.5",
    "@angular-eslint/eslint-plugin-template": "^18.0.0-alpha.5",
    "@angular-eslint/schematics": "^18.0.0-alpha.5",
    "@angular-eslint/template-parser": "^18.0.0-alpha.5",
    "@angular/cli": "^18.0.0",
    "@angular/compiler-cli": "~18.0.0",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.56.0",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "pretty-quick": "^3.1.3",
    "typescript": "~5.4.5",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "optionalDependencies": {
    "@nx/nx-darwin-arm64": "17.2.7",
    "@nx/nx-darwin-x64": "17.2.7",
    "@nx/nx-linux-x64-gnu": "17.2.7"
  }
}
